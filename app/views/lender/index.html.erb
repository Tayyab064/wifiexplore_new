<script type="text/javascript">
  var mas = [];
  <% @wifis.each do |wifi| %>
	mas.push({latLng: [<%= wifi.latitude %>, <%= wifi.longitude %>], name: '<%= wifi.name %>'});
  <% end %>

  $('#world_map').vectorMap({
      map: 'world_mill_en',
      normalizeFunction: 'polynomial',
      backgroundColor: '#fff',            
      regionStyle: {
        initial: {
          fill: '#36424f'
        },
       hover: {
          fill: '#177bbb'
        },
      },
      markerStyle: {
        initial: {
          fill: '#1ccc88',
          stroke: '#fff'
        }
      },
      markers: mas
    });
</script>


