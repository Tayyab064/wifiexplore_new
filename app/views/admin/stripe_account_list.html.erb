

<div class="page-content-wrapper ">
<!-- START PAGE CONTENT -->
  <div class="content">
    <!-- START CONTAINER FLUID -->
    <div class="container-fluid container-fixed-lg bg-white">
      <!-- START PANEL -->
      <div class="panel panel-transparent">
        <div class="panel-heading">
          <div class="panel-title">Paypal
          </div>
          <div class="pull-right">
            <div class="col-xs-12">
              <input type="text" id="search-table" class="form-control pull-right" placeholder="Search">
            </div>
          </div>
          <div class="clearfix"></div>
        </div>
        <div class="panel-body">
          <table class="table table-hover demo-table-search" id="tableWithSearch">
            <thead> 
				<tr>
					<th>Date</th>
					<th >Amount</th>
					<th>Name</th>	 
					<th>Paypal Email</th> 
					<th>Email</th>
					<th>Status</th>
				</tr> 
			</thead> 
			<tbody> 
				<% @str.each do |stripe| %>
					<tr>
						<td><%= stripe.created_at.strftime('%H:%M %m/%d/%Y') %></td>
						<td><%= stripe.amount/100 %></td>
						<td><%= stripe.lender.name %></td>
						<td><%= stripe.paypalemail %></td>
						<td><%= stripe.lender.email %></td>
						<td><%= stripe.transfered ? 'Transfered' : 'Pending' %></td>
					</tr>
				<% end %>						
			</tbody>  
          </table>
        </div>
      </div>
      <!-- END PANEL -->
    </div>
    <!-- END CONTAINER FLUID -->
  </div>
</div>